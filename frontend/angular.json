{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "cli": {
        "packageManager": "npm",
        "schematicCollections": [
            "angular-eslint"
        ],
        "analytics": false
    },
    "newProjectRoot": "projects",
    "projects": {
        "shell": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/shell",
            "sourceRoot": "projects/shell/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-architects/native-federation:build",
                    "options": {},
                    "configurations": {
                        "production": {
                            "target": "shell:esbuild:production"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-architects/native-federation:build",
                    "options": {
                        "target": "shell:serve-original:production",
                        "rebuildDelay": 0,
                        "dev": true,
                        "port": 0,
                        "host": "0.0.0.0"
                    },
                    "configurations": {
                        "production": {
                            "target": "shell:serve-original:production"
                        }
                    }
                },
                "esbuild": {
                    "builder": "@angular/build:application",
                    "options": {
                        "browser": "projects/shell/src/main.ts",
                        "polyfills": [
                            "es-module-shims"
                        ],
                        "tsConfig": "projects/shell/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "projects/shared/i18n",
                                "output": "/shared/i18n"
                            },
                            {
                                "glob": "**/*",
                                "input": "projects/shell/src/app/i18n",
                                "output": "/assets/shell/i18n"
                            },
                            "projects/shell/src/favicon.ico",
                            "projects/shell/src/assets",
                            "projects/shell/src/environments/federation.manifest.json"
                        ],
                        "styles": [
                            "projects/shell/src/styles.scss"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "8kB",
                                    "maximumError": "10kB"
                                }
                            ],
                            "optimization": false,
                            "sourceMap": true,
                            "outputHashing": "all"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve-original": {
                    "builder": "@angular/build:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "shell:esbuild:production"
                        }
                    },
                    "defaultConfiguration": "production",
                    "options": {
                        "port": 4200,
                        "host": "0.0.0.0",
                        "proxyConfig": "projects/shell/proxy.conf.json"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/shell/src/**/*.ts",
                            "projects/shell/src/**/*.html"
                        ]
                    }
                },
                "test": {
                    "builder": "@angular/build:unit-test"
                }
            }
        },
